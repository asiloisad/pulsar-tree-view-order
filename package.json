{
  "name": "tree-view-order",
  "description": "Select sort method of tree-view",
  "repository": "https://github.com/asiloisad/pulsar-tree-view-order",
  "keywords": [
    "tree-view",
    "order",
    "sort",
    "natural"
  ],
  "version": "1.1.1",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "dependencies": {
    "natsort": "^2.0.3",
    "natural-compare-lite": "^1.4.0"
  },
  "configSchema": {
    "sortMethod": {
      "order": 1,
      "title": "Sort method",
      "description": "Select the tree view sorting method.",
      "type": "integer",
      "enum": [
        { "value": 0, "description": "Original tree-view" },
        { "value": 1, "description": "Natural sorting: alphabetical and numbers" },
        { "value": 2, "description": "Natural sorting: by character code and numbers" }
      ],
      "default": 1
    }
  }
}
